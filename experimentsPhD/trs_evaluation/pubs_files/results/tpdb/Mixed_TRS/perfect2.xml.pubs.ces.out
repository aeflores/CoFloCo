CRS $pubs_aux_entry$(A,B,C,D) -- THE MAIN ENTRY

  * Non Asymptotic Upper Bound: max([c(failed(schemes_failed,[scc=3,cr=f/4])),1+c(failed(schemes_failed,[scc=3,cr=f/4])),2+nat(A),2+nat(A)]) 

  * LOOPS $pubs_aux_entry$(E,F,G,H) -> $pubs_aux_entry$(I,J,K,L)

  * Ranking function: N/A

  * Invariants $pubs_aux_entry$(A,B,C,D) -> $pubs_aux_entry$(E,F,G,H)

     entry  : []
     non-rec: [A=E,B=F,C=G,D=H]
     rec    : [0=1]
     inv    : [A=E,B=F,C=G,D=H]

CRS start(A,B,C,D)

  * Non Asymptotic Upper Bound: max([c(failed(schemes_failed,[scc=3,cr=f/4])),1+c(failed(schemes_failed,[scc=3,cr=f/4])),2+nat(A),2+nat(A)]) 

  * LOOPS start(E,F,G,H) -> start(I,J,K,L)

  * Ranking function: N/A

  * Invariants start(A,B,C,D) -> start(E,F,G,H)

     entry  : []
     non-rec: [A=E,B=F,C=G,D=H]
     rec    : [0=1]
     inv    : [A=E,B=F,C=G,D=H]

CRS perfectp(A)

  * Non Asymptotic Upper Bound: 1+c(failed(schemes_failed,[scc=3,cr=f/4])) 

  * LOOPS perfectp(B) -> perfectp(C)

  * Ranking function: N/A

  * Invariants perfectp(A) -> perfectp(B)

     entry  : [A>=0]
     non-rec: [A=B,A>=0]
     rec    : [0=1]
     inv    : [A>=0,A=B]

CRS f(A,B,C,D)

  * Non Asymptotic Upper Bound: c(failed(schemes_failed,[scc=3,cr=f/4])) 

  * LOOPS f(E,F,G,H) -> f(I,J,K,L)
     - [E+K>=G,E>=1,G>=K,K>=0,H>=0,H=L,F=0,E=I+1,H=J]
     - [F>=J+1,E+J>=F,G>=0,H>=0,J>=0,H=L,E=I,G=K]
     - [E>=1,F>=1,G>=0,H>=0,G=K,H=J,E=I+1,H=L]

  * Ranking function: c(failed(no_rf,[scc=3,cr=f/4]))

  * Invariants f(A,B,C,D) -> f(E,F,G,H)

     entry  : [D>=0,C>=0,B>=0,A>=0]
     non-rec: [A=E,B=F,C=G,D=H,D>=0,C>=0,B>=0,A>=0]
     rec    : [D=H,G>=0,F>=0,D>=0,B+D>=F,B>=0,E>=0,C>=G,A>=1,A+B>=E+1,A>=E,A+B+D>=E+F+1]
     inv    : [B>=0,B+D>=F,E>=0,A>=E,D>=0,F>=0,G>=0,C>=G,D=H]

CRS minus(A,B)

  * Non Asymptotic Upper Bound: 2+nat(A) 

  * LOOPS minus(C,D) -> minus(E,F)
     - [D=F+1,C=E+1,C>=1,D>=1]

  * Ranking function: nat(D)

  * Invariants minus(A,B) -> minus(C,D)

     entry  : [B>=0,A>=0]
     non-rec: [A=C,B=D,B>=0,A>=0]
     rec    : [A+D=B+C,C>=0,B+C>=A,A>=C+1]
     inv    : [B>=D,A+D>=B,D>=0,A+D=B+C]

CRS le(A,B)

  * Non Asymptotic Upper Bound: 2+nat(A) 

  * LOOPS le(C,D) -> le(E,F)
     - [D=F+1,C=E+1,D>=1,C>=1]

  * Ranking function: nat(D)

  * Invariants le(A,B) -> le(C,D)

     entry  : [B>=0,A>=0]
     non-rec: [A=C,B=D,B>=0,A>=0]
     rec    : [A+D=B+C,C>=0,B+C>=A,A>=C+1]
     inv    : [B>=D,A+D>=B,D>=0,A+D=B+C]

CRS if(A,B,C)

  * Non Asymptotic Upper Bound: 1 

  * LOOPS if(D,E,F) -> if(G,H,I)

  * Ranking function: N/A

  * Invariants if(A,B,C) -> if(D,E,F)

     entry  : [1>=A,C>=0,B>=0,A>=0]
     non-rec: [A=D,B=E,C=F,1>=A,C>=0,B>=0,A>=0]
     rec    : [0=1]
     inv    : [B>=0,C>=0,A>=0,1>=A,A=D,B=E,C=F]


