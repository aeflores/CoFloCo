CRS $pubs_aux_entry$(A,B,C) -- THE MAIN ENTRY

  * Non Asymptotic Upper Bound: max([1+nat(A),pow(2,nat(1*A+1*B+ -1))-1+3*pow(2,nat(1*A+1*B+ -1))-1]) 

  * LOOPS $pubs_aux_entry$(D,E,F) -> $pubs_aux_entry$(G,H,I)

  * Ranking function: N/A

  * Invariants $pubs_aux_entry$(A,B,C) -> $pubs_aux_entry$(D,E,F)

     entry  : []
     non-rec: [A=D,B=E,C=F]
     rec    : [0=1]
     inv    : [A=D,B=E,C=F]

CRS start(A,B,C)

  * Non Asymptotic Upper Bound: max([1+nat(A),pow(2,nat(1*A+1*B+ -1))-1+3*pow(2,nat(1*A+1*B+ -1))-1]) 

  * LOOPS start(D,E,F) -> start(G,H,I)

  * Ranking function: N/A

  * Invariants start(A,B,C) -> start(D,E,F)

     entry  : []
     non-rec: [A=D,B=E,C=F]
     rec    : [0=1]
     inv    : [A=D,B=E,C=F]

CRS merge(A,B)

  * Non Asymptotic Upper Bound: pow(2,nat(A+B-1))-1+3*pow(2,nat(A+B-1))-1 

  * LOOPS merge(C,D) -> merge(E,F)
     - [D>=1,C>=E+1,E>=0,D=F]
     - [D>=F+1,F>=0,C>=1,C=E]

  * Ranking function: nat(C+D-1)

  * Invariants merge(A,B) -> merge(C,D)

     entry  : [B>=0,A>=0]
     non-rec: [A=C,B=D,B>=0,A>=0]
     rec    : [B>=D,C>=0,A+B>=C+D+1,A>=1,C+D>=1,B>=1,D>=0,A>=C]
     inv    : [C>=0,B>=D,A>=C,D>=0]

CRS if(A,B,C)

  * Non Asymptotic Upper Bound: 1 

  * LOOPS if(D,E,F) -> if(G,H,I)

  * Ranking function: N/A

  * Invariants if(A,B,C) -> if(D,E,F)

     entry  : [C>=0,B>=0,A>=0]
     non-rec: [A=D,B=E,C=F,C>=0,B>=0,A>=0]
     rec    : [0=1]
     inv    : [A>=0,B>=0,C>=0,A=D,B=E,C=F]

CRS fun(A,B)

  * Non Asymptotic Upper Bound: 1+nat(A) 

  * LOOPS fun(C,D) -> fun(E,F)
     - [D=F,E>=0,C>=E+1,D>=0]

  * Ranking function: nat(C)

  * Invariants fun(A,B) -> fun(C,D)

     entry  : [B>=0,A>=0]
     non-rec: [A=C,B=D,B>=0,A>=0]
     rec    : [B>=0,A>=C+1,C>=0,B=D]
     inv    : [A>=C,C>=0,B>=0,B=D]


