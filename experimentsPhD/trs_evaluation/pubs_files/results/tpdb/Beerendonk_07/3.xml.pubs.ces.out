CRS $pubs_aux_entry$(A,B,C) -- THE MAIN ENTRY

  * Non Asymptotic Upper Bound: max([1+nat(A),2+nat(A),c(failed(schemes_failed,[scc=2,cr=cond/3]))]) 

  * LOOPS $pubs_aux_entry$(D,E,F) -> $pubs_aux_entry$(G,H,I)

  * Ranking function: N/A

  * Invariants $pubs_aux_entry$(A,B,C) -> $pubs_aux_entry$(D,E,F)

     entry  : []
     non-rec: [A=D,B=E,C=F]
     rec    : [0=1]
     inv    : [A=D,B=E,C=F]

CRS start(A,B,C)

  * Non Asymptotic Upper Bound: max([1+nat(A),2+nat(A),c(failed(schemes_failed,[scc=2,cr=cond/3]))]) 

  * LOOPS start(D,E,F) -> start(G,H,I)

  * Ranking function: N/A

  * Invariants start(A,B,C) -> start(D,E,F)

     entry  : []
     non-rec: [A=D,B=E,C=F]
     rec    : [0=1]
     inv    : [A=D,B=E,C=F]

CRS cond(A,B,C)

  * Non Asymptotic Upper Bound: c(failed(schemes_failed,[scc=2,cr=cond/3])) 

  * LOOPS cond(D,E,F) -> cond(G,H,I)
     - [F>=0,E>=G,1>=G,G>=0,E=H,D=1,E+F=I]

  * Ranking function: c(failed(no_rf,[scc=2,cr=cond/3]))

  * Invariants cond(A,B,C) -> cond(D,E,F)

     entry  : [C>=0,B>=0,A>=0]
     non-rec: [A=D,B=E,C=F,C>=0,B>=0,A>=0]
     rec    : [B=E,A=1,C>=0,1>=D,B>=D,D>=0,F>=B+C]
     inv    : [F>=C,B>=0,D>=0,D+1>=A,C>=0,A>=D,B=E]

CRS gr(A,B)

  * Non Asymptotic Upper Bound: 2+nat(A) 

  * LOOPS gr(C,D) -> gr(E,F)
     - [D=F+1,C=E+1,D>=1,C>=1]

  * Ranking function: nat(D)

  * Invariants gr(A,B) -> gr(C,D)

     entry  : [B>=0,A>=0]
     non-rec: [A=C,B=D,B>=0,A>=0]
     rec    : [A+D=B+C,C>=0,B+C>=A,A>=C+1]
     inv    : [B>=D,A+D>=B,D>=0,A+D=B+C]

CRS add(A,B)

  * Non Asymptotic Upper Bound: 1+nat(A) 

  * LOOPS add(C,D) -> add(E,F)
     - [D=F,C=E+1,E>=0,D>=0]

  * Ranking function: nat(C)

  * Invariants add(A,B) -> add(C,D)

     entry  : [B>=0,A>=0]
     non-rec: [A=C,B=D,B>=0,A>=0]
     rec    : [B=D,B>=0,C>=0,A>=C+1]
     inv    : [A>=C,C>=0,B>=0,B=D]


