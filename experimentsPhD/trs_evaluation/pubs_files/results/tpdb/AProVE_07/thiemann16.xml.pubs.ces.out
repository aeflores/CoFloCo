CRS $pubs_aux_entry$(A,B,C,D,E) -- THE MAIN ENTRY

  * Non Asymptotic Upper Bound: max([c(failed(schemes_failed,[scc=4,cr=if/5])),5+nat(1*A+ -1)/2+nat(C)+c(failed(schemes_failed,[scc=4,cr=if/5])),6+nat(1*A+ -1)/2+c(failed(schemes_failed,[scc=4,cr=if/5])),1+nat(A),2+nat(1*A)/2,3+nat(1*A+ -1)/2]) 

  * LOOPS $pubs_aux_entry$(F,G,H,I,J) -> $pubs_aux_entry$(K,L,M,N,O)

  * Ranking function: N/A

  * Invariants $pubs_aux_entry$(A,B,C,D,E) -> $pubs_aux_entry$(F,G,H,I,J)

     entry  : []
     non-rec: [A=F,B=G,C=H,D=I,E=J]
     rec    : [0=1]
     inv    : [A=F,B=G,C=H,D=I,E=J]

CRS start(A,B,C,D,E)

  * Non Asymptotic Upper Bound: max([c(failed(schemes_failed,[scc=4,cr=if/5])),5+nat(1*A+ -1)/2+nat(C)+c(failed(schemes_failed,[scc=4,cr=if/5])),6+nat(1*A+ -1)/2+c(failed(schemes_failed,[scc=4,cr=if/5])),1+nat(A),2+nat(1*A)/2,3+nat(1*A+ -1)/2]) 

  * LOOPS start(F,G,H,I,J) -> start(K,L,M,N,O)

  * Ranking function: N/A

  * Invariants start(A,B,C,D,E) -> start(F,G,H,I,J)

     entry  : []
     non-rec: [A=F,B=G,C=H,D=I,E=J]
     rec    : [0=1]
     inv    : [A=F,B=G,C=H,D=I,E=J]

CRS times(A,B)

  * Non Asymptotic Upper Bound: 6+nat(1*A+ -1)/2+c(failed(schemes_failed,[scc=4,cr=if/5])) 

  * LOOPS times(C,D) -> times(E,F)

  * Ranking function: N/A

  * Invariants times(A,B) -> times(C,D)

     entry  : [B>=0,A>=0]
     non-rec: [A=C,B=D,B>=0,A>=0]
     rec    : [0=1]
     inv    : [A>=0,B>=0,A=C,B=D]

CRS if(A,B,C,D,E)

  * Non Asymptotic Upper Bound: c(failed(schemes_failed,[scc=4,cr=if/5])) 

  * LOOPS if(F,G,H,I,J) -> if(K,L,M,N,O)
     - [L+1>=G,K>=0,G>=L,2>=K,I>=0,H>=0,K+L>=2,J>=0,H=M,H+J=O,F=1,J=N]
     - [K>=0,K+L>=2,2*H+I>=2*O,2*L+1>=G,2>=K,2*O+1>=2*H+I,G>=2*L,J>=0,O>=H,H>=0,H+N=O,F=0,H=M]
     - [2*N>=I,2*L+1>=G,K>=0,I>=0,K+L>=2,I+1>=2*N,G>=2*L,2>=K,J>=0,O>=N,M+N=O,F=0,H+N=O]

  * Ranking function: c(failed(no_rf,[scc=4,cr=if/5]))

  * Invariants if(A,B,C,D,E) -> if(F,G,H,I,J)

     entry  : [2>=A,B>=0,C>=0,E>=0,D>=0,A>=0]
     non-rec: [A=F,B=G,C=H,D=I,E=J,2>=A,B>=0,C>=0,E>=0,D>=0,A>=0]
     rec    : [C+I=J,C=H,E>=0,D>=0,C>=0,F+G>=2,2>=F,1>=A,F>=0,I>=0,B>=G,A>=0]
     inv    : [F+2>=2*A,2*F+G>=2*A,F>=0,A>=0,2>=F,G>=0,C+E+I>=J,J>=0,C>=0,D+J>=I,I>=0,E>=0,D>=0,B>=G,C=H]

CRS plus(A,B)

  * Non Asymptotic Upper Bound: 1+nat(A) 

  * LOOPS plus(C,D) -> plus(E,F)
     - [D=F,C=E+1,E>=0,D>=0]

  * Ranking function: nat(C)

  * Invariants plus(A,B) -> plus(C,D)

     entry  : [B>=0,A>=0]
     non-rec: [A=C,B=D,B>=0,A>=0]
     rec    : [B=D,B>=0,C>=0,A>=C+1]
     inv    : [A>=C,C>=0,B>=0,B=D]

CRS p(A)

  * Non Asymptotic Upper Bound: 1 

  * LOOPS p(B) -> p(C)

  * Ranking function: N/A

  * Invariants p(A) -> p(B)

     entry  : [A>=0]
     non-rec: [A=B,A>=0]
     rec    : [0=1]
     inv    : [A>=0,A=B]

CRS half(A)

  * Non Asymptotic Upper Bound: 2+nat(A)/2 

  * LOOPS half(B) -> half(C)
     - [B=C+2,B>=2]

  * Ranking function: nat(B/2)

  * Invariants half(A) -> half(B)

     entry  : [A>=0]
     non-rec: [A=B,A>=0]
     rec    : [B>=0,A>=B+2]
     inv    : [A>=B,B>=0]

CRS check(A)

  * Non Asymptotic Upper Bound: 3+nat(A-1)/2 

  * LOOPS check(B) -> check(C)
     - [B=C+2,B>=3]

  * Ranking function: nat(B/2-1/2)

  * Invariants check(A) -> check(B)

     entry  : [A>=0]
     non-rec: [A=B,A>=0]
     rec    : [B>=1,A>=B+2]
     inv    : [B>=0,A>=B]


